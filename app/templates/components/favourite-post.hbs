<ul>
    {{#each this.favPosts.posts as |post|}}
    <li class="post-data">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">{{post.title}}</h5>
                <p class="card-text">{{post.body}}</p>
            </div>
        </div>
    </li>
    {{/each}}
</ul>